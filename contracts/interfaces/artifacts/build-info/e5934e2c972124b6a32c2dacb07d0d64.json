		"sources": {
			"contracts/interfaces/IMargari.sol": {
				"content": "// SPDX-License-Identifier: APACHE 2.0\r\npragma solidity ^0.8.23;\r\n\r\nimport {Gilt, GiltClaim} from \"./IMStructs.sol\";\r\n\r\ninterface IMargari {\r\n\r\n    // send a gilt to a destination chain \r\n    function sendGilt(Gilt memory _gilt, bytes32 _alloProjectId, uint256 _destinationChainId) external returns (uint256 _sendId);\r\n\r\n    // this function sends from the given project Id to the funds to the given remote recipient on the given chain\r\n    function sendFunds(uint256 _chainId, address erc20, uint256 amount, address _remoteRecipient, bytes32 _alloProjectId) external payable returns (uint256 _sendId);\r\n\r\n    // this function enables the recipient to claim their gilts \r\n    function claimGilts() external returns (GiltClaim [] memory _giltClaims);\r\n\r\n}"
			},
			"contracts/interfaces/IMStructs.sol": {
				"content": "// SPDX-License-Identifier: APACHE 2.0\r\npragma solidity ^0.8.23;\r\n\r\n\r\nstruct Gilt {\r\n    address erc20; \r\n    uint256 amount; \r\n    uint256 creationDate; \r\n    uint256 chainId; \r\n    address giltContract; \r\n    uint256 id; \r\n}\r\n\r\nstruct TransmittedGilt { \r\n    Gilt gilt; \r\n    bytes32 projectId;\r\n    address owner; \r\n}\r\n\r\nstruct GiltClaim { \r\n    bytes32 projectId; \r\n    uint256 giltId; \r\n    uint256 chainId; \r\n}\r\n\r\nstruct DestinationConfig {\r\n    uint256 chainId; \r\n    uint64 ccipChainSelector; \r\n    address routerAddress; \r\n    address [] feeTokens; \r\n    address giltReciever; \r\n}\r\n\r\nstruct SendStatement { \r\n    uint256 sendId;\r\n    uint256 fees; \r\n    address token; \r\n    uint256 sendDate;\r\n    bytes32 ccipMesssageId;\r\n}"
			}
		},
